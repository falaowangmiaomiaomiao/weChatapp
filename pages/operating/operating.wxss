page {
  width: 100%;
  height: 100%;
}
.container {
  box-sizing: border-box;
}
.header {
  position: fixed;
  top: 0;
  width: 100%;
  background: #5490fe;
  z-index: 1000
}
.status-bar {
  background: #5490fe;
}
.title-bar {
  display: flex;
  justify-content: space-between;
  align-items: center;
  text-align: center;
  color:#fff;
  font-size: 36rpx;
  font-weight: 600;
}
.title-bar .back-btn, .title-bar .tablet {
  width: 116px; /* 保持一致使标题居中 */
}
.title-bar .tablet { /* 可优化 */
  /* box-sizing: border-box; */
  /* padding-left: 1em; */
}
.tab-box{
    width: 100%;
    text-align: center;
    height: 84rpx;
    line-height: 84rpx;
    display: flex;
    justify-content: space-between;
    font-size: 28rpx;
}
.tab-item{
    width: 50%; 
    color:#161616;
    background: #fbfbfb;
}
.active{
    color:#5490fe;
    border-bottom: 2rpx solid #5490fe;
}
.tab-items{
  width: 100%;
  height: 80%;
  text-align: center;
}
.show{
  width: 100%;
  height: auto;
  overflow: scroll;
  display: block
}
.hidden{display: none}
.popup{
  width: 100%;
